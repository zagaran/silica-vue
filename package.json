{
  "name": "silica-vue",
  "version": "0.0.10",
  "description": "A Vue implementation of JSONForms.io's component library specifically to support rendering of django-silica components.",
  "author": "NoahHoughton",
  "private": false,
  "repository": "zagaran/silica-vue",
  "bugs": {
     "url": "https://github.com/zagaran/silica-vue/issues"
   },
  "homepage": "https://github.com/zagaran/silica-vue",
  "main": "dist/vue-example-pkg.umd.min.js",
  "unpkg": "dist/silica-vue.umd.min.js",
  "jsdelivr": "dist/silica-vue.umd.min.js",
  "files": [
   "dist/*",
   "src/*"
 ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "vue-cli-service build src/export.js --target lib --name silica-vue",
    "prepublishOnly": "$npm_execpath build"
  },
  "dependencies": {
    "@jsonforms/core": "^3.0.0-beta.1",
    "@jsonforms/vue2": "^3.0.0-beta.1",
    "@jsonforms/vue2-vanilla": "^3.0.0-beta.1"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/composition-api": "^1.0.0-rc.3",
    "core-js": "^3.21.1",
    "js-cookie": "^3.0.1",
    "process": "^0.11.10",
    "vue-template-compiler": "^2.6.14"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue2",
    "django",
    "jsonforms"
  ]
}
