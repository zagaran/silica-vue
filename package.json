{
  "name": "silica-vue",
  "version": "0.0.12",
  "description": "A Vue implementation of JSONForms.io's component library specifically to support rendering of django-silica components.",
  "author": "NoahHoughton",
  "private": false,
  "repository": "zagaran/silica-vue",
  "bugs": {
    "url": "https://github.com/zagaran/silica-vue/issues"
  },
  "homepage": "https://github.com/zagaran/silica-vue",
  "files": [
    "dist"
  ],
  "main": "dist/silica-vue.umd.js",
  "module": "dist/silica-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/silica-vue.es.js",
      "require": "./dist/silica-vue.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "$npm_execpath run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@jsonforms/core": "^3.0.0-beta.1",
    "@jsonforms/vue2": "^3.0.0-beta.1",
    "@jsonforms/vue2-vanilla": "^3.0.0-beta.1"
  },
  "peerDependencies": {
    "vue": "^2.6.14",
    "@vue/composition-api": "^1.4.9"
  },
  "devDependencies": {
    "core-js": "^3.21.1",
    "js-cookie": "^3.0.1",
    "process": "^0.11.10",
    "unplugin-auto-import": "^0.6.1",
    "vite": "^2.8.6",
    "vite-plugin-vue2": "^1.9.3"
  }
}
