{
  "name": "silica-vue",
  "description": "A Vue implementation of JSONForms.io's component library specifically to support rendering of django-silica components.",
  "author": "NoahHoughton",
  "version": "0.0.9",
  "repository": "zagaran/silica-vue",
  "homepage": "https://github.com/zagaran/silica-vue",
  "main": "dist/silica-vue.ssr.js",
  "browser": "dist/silica-vue.esm.js",
  "module": "dist/silica-vue.esm.js",
  "unpkg": "dist/silica-vue.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "js-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@jsonforms/core": "^2.5.2",
    "@jsonforms/vue2": "^2.5.2",
    "@jsonforms/vue2-vanilla": "^2.5.2",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/composition-api": "^1.0.0-rc.3",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  }
}
