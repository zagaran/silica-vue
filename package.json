{
  "name": "silica-vue",
  "version": "0.1.0-alpha.10",
  "description": "A Vue implementation of JSONForms.io's component library specifically to support rendering of silica-django components.",
  "author": "NoahHoughton",
  "private": false,
  "repository": "zagaran/silica-vue",
  "browserslist": [
    "> 0.5%",
    "not dead",
    "ie >= 11"
  ],
  "bugs": {
    "url": "https://github.com/zagaran/silica-vue/issues"
  },
  "homepage": "https://github.com/zagaran/silica-vue",
  "files": [
    "dist"
  ],
  "main": "dist/silica-vue.umd.js",
  "module": "dist/silica-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/silica-vue.es.js",
      "require": "./dist/silica-vue.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "$npm_execpath run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@jsonforms/core": "^3.0.0-rc.1",
    "@jsonforms/vue2": "^3.0.0-rc.1",
    "@jsonforms/vue2-vanilla": "^3.0.0-rc.1",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "vue-template-compiler": "^2.7.10"
  },
  "peerDependencies": {
    "vue": "^2.7.10"
  },
  "devDependencies": {
    "core-js": "^3.21.1",
    "unplugin-auto-import": "^0.6.1",
    "vite": "^2.8.6",
    "vite-plugin-vue2": "^1.9.3",
    "vue": "^2.7.10",
    "vue-json-viewer": "^2.2.22"
  }
}
